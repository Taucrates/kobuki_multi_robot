<!-- Launches Kobukis Gazebo simulation in a world -->
<launch>
  <arg name="world_label" default="srvlab"/>

  <!-- start Gazebo -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <env name="GAZEBO_MODEL_PATH" value="$(find kobuki_multi_robot)/simulation/environment/models"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="debug" value="false"/>
    <arg name="world_name" value="$(find kobuki_multi_robot)/simulation/environment/worlds/$(arg world_label).world"/>
  </include>
</launch>